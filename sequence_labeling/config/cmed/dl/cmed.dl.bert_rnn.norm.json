{
  "data_name": "cmed",
  "data_root": "./Datasets/cmed/",
  "inputdata_file_name": "data.input",
  "outputdata_file_name": "data.output",
  "dict_file_name": "vocab.json",
  "tags_file_name": "tags.json",
  "model_save_name": "model.ckpt",

  "n_folds": 10,
  "split_rate": 0.7,
  "cover_rate": 1,
  "min_count": 0,
  "shuffle": 1,
  "feature": "norm",
  "pad_token": "[PAD]",

  "model_name": "Bert_RNN",
  "checkpoint": "bert-base-chinese",
  "embedding_dim": 768,
  "hidden_dim": 128,
  "num_layers": 1,
  "bidirectional": "True",
  "dropout_rate": 0.3,

  "num_epochs": 10,
  "batch_size": 32,
  "learning_rate": 1e-3,
  "criterion_name": "NLLLoss",
  "optimizer_name": "Adam"
}